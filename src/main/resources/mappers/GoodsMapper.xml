<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.panda.mapper.GoodsMapper">
	
	<!-- 중고상품 등록 -->
	<insert id="createGoods">
		insert into goods(goods_image,goods_category,goods_title,goods_condition,goods_price,goods_discount,goods_detail,goods_like,goods_donation)
		values (#{goods_image},#{goods_category},#{goods_title},#{goods_condition},#{goods_price},#{goods_discount},#{goods_detail},#{goods_like},#{goods_donation})
	</insert> 
	
	
	


</mapper>